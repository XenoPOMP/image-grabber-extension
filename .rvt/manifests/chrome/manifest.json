{
  "manifest_version": 3,
  "name": "Grab the Images",
  "description": "Small extension for grabbing all images from page.",
  "version": "1.0.0",
  "default_locale": "en",
  "action": {
    "default_popup": "index.html",
    "default_title": "Grab the Images"
  },
  "permissions": [
    "activeTab",
    "tabGroups",
    "tabs",
    "downloads",
    "scripting"
  ],
  "optional_permissions": [],
  "host_permissions": [
    "https://*/"
  ],
  "optional_host_permissions": []
}