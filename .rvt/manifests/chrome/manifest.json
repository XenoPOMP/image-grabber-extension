{
  "manifest_version": 3,
  "name": "Grab the Images",
  "description": "Small extension for grabbing all images from page.",
  "version": "1.0.0",
  "default_locale": "en",
  "action": {
    "default_popup": "index.html",
    "default_title": "Grab the Images"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png",
    "256": "icons/icon256.png",
    "512": "icons/icon512.png"
  },
  "permissions": [
    "activeTab",
    "tabGroups",
    "tabs",
    "downloads",
    "scripting"
  ],
  "optional_permissions": [],
  "host_permissions": [
    "https://*/"
  ],
  "optional_host_permissions": []
}